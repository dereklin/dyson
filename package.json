{
  "name": "dyson",
  "version": "2.0.6",
  "description": "Node server for dynamic, fake JSON.",
  "keywords": [
    "API",
    "JSON",
    "REST",
    "data",
    "dummy",
    "dynamic",
    "fake",
    "generator",
    "proxy",
    "response",
    "server"
  ],
  "author": {
    "email": "lars@webpro.nl",
    "name": "Lars Kappert"
  },
  "main": "./lib/dyson.js",
  "bin": {
    "dyson": "./bin/dyson.js"
  },
  "scripts": {
    "test": "bron test/*.js",
    "lint": "eslint lib test",
    "format": "prettier --write \"{bin,lib,test}/**/*.js\"",
    "release": "release-it"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/webpro/dyson"
  },
  "homepage": "https://github.com/webpro/dyson#readme",
  "bugs": "https://github.com/webpro/dyson/issues",
  "files": [
    "bin",
    "lib"
  ],
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "body-parser": "1.19.0",
    "cookie-parser": "1.4.4",
    "cors": "2.8.5",
    "express": "4.17.1",
    "lodash": "4.17.14",
    "request": "2.88.0",
    "require-directory": "2.1.1",
    "serve-favicon": "2.5.0"
  },
  "devDependencies": {
    "@babel/plugin-syntax-object-rest-spread": "7.2.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "bron": "1.0.2",
    "dyson-generators": "0.2.0",
    "dyson-image": "0.2.0",
    "eslint": "6.0.1",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-prettier": "3.1.0",
    "prettier": "1.18.2",
    "release-it": "12.3.3",
    "sinon": "7.3.2",
    "supertest": "4.0.2"
  },
  "ava": {
    "babel": {
      "testOptions": {
        "presets": [
          "@ava/stage-4",
          "@ava/transform-test-files"
        ],
        "plugins": [
          "transform-object-rest-spread"
        ]
      }
    }
  },
  "license": "MIT"
}
